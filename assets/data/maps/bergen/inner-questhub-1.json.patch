[
	{ "type": "ENTER", "index": "entities" },
	{ 
		"type": "IF", 
		"_comment": "Adds corresponding patch if full Quest Hub is on", 
		"cond": "sc.options.get('openworld-fullQuestHub')", 
		"thenSteps": [
				{
						"type": "ENTER",
						"index": [114,"settings","interact", "event"]
				},
				{ 
						"_comment": "Replace Quest Hub", 
						"type": "SET_KEY", 
						"index": 3,
						"content": {
								"hub": "bergen",
								"type": "OPEN_QUEST_HUB_FULL"
						}
				},
				{ "type": "EXIT", "count": 4 }
		]
	},
	{ "type": "EXIT" }
]